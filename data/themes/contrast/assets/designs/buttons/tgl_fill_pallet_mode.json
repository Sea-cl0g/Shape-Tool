{
    "containerAnker": "topRight",
    "blockMode": "vertical",
    "blockAnker": "CENTER",
    "layer": 2,
    "colors": {
        "defaultCol": "#4a7083ff"
    },
    "floats": {
        "x_pos": 4,
        "y_pos": 4,
        "b_size": 2
    },
    "layouts": {
        "buttonLayout": {
            "x_point": "$x_pos",
            "y_point": 3.7,
            "width_point": "$b_size",
            "height_point": "$b_size",
            "r_point": 0.7
        }
    },
    "images": {
        "main": {
            "scale": 0.8,
            "path": "icon/23.svg",
            "color": "#fffff0"
        }
    },
    "shadows": {
        "main": {
            "shadowMode": "center",
            "shadowDistPoint": 0.2,
            "color": "#222222"
        }
    },
    "strokes": {
        "buttonStroke": {
            "stroke_point": 0.1,
            "color": "#fffff0"
        }
    },
    "elements": {
        "@FILL_BUTTON": {
            "type": "button",
            "style": [
                {
                    "predicate": "normal",
                    "layout": "$buttonLayout",
                    "fillCol": "$defaultCol",
                    "stroke": "$buttonStroke",
                    "image": "$main"
                },
                {
                    "predicate": "touched",
                    "layout": "$buttonLayout",
                    "fillCol": "$defaultCol",
                    "stroke": {
                        "stroke_point": 0.1,
                        "color": "#ffffff"
                    },
                    "image": "$main"
                },
                {
                    "predicate": "clicked",
                    "layout": "$buttonLayout",
                    "fillCol": "$defaultCol",
                    "stroke": "$buttonStroke",
                    "image": "$main"
                }
            ]
        },
        "e1": {
            "type": "text_block",
            "layout": {
                "x_point": "$x_pos",
                "y_point": "$y_pos",
                "width_point": 6,
                "height_point": 1
            },
            "text": {
                "text": "fill",
                "text_align": "LEFT",
                "text_size": 0.6,
                "text_col": "#FFFFF0"
            },
            "fillCol": "#FFFFFF00"
        }
    }
}