{
    "containerAnker": "topRight",
    "blockMode": "vertical",
    "blockAnker": "CENTER",
    "layer": 10,
    "colors": {
        "noFill": "#FFFFFF00"
    },
    "floats": {
        "x_pos": 9,
        "y_pos": 8
    },
    "elements": {
        "@STROKE_HSB_H": {
            "type": "color_picker",
            "layout": {
                "x_point": "$x_pos",
                "y_point": "$y_pos - 1",
                "width_point": 6,
                "height_point": 0.8
            }
        },
        "@STROKE_HSB_S": {
            "type": "color_picker",
            "layout": {
                "x_point": "$x_pos",
                "y_point": "$y_pos",
                "width_point": 6,
                "height_point": 0.8
            }
        },
        "@STROKE_HSB_B": {
            "type": "color_picker",
            "layout": {
                "x_point": "$x_pos",
                "y_point": "$y_pos + 1",
                "width_point": 6,
                "height_point": 0.8
            }
        },
        "@STROKE_ALPHA": {
            "type": "color_picker",
            "layout": {
                "x_point": "$x_pos",
                "y_point": "$y_pos + 2",
                "width_point": 6,
                "height_point": 0.8
            }
        },
        "e1": {
            "type": "text_block",
            "layout": {
                "x_point": "$x_pos",
                "y_point": "$y_pos - 3 + 0.8",
                "width_point": 7,
                "height_point": 1
            },
            "text": {
                "text": "stroke",
                "text_align": "CENTER",
                "text_size": 0.7,
                "text_col": "#000030"
            },
            "fillCol": "$noFill"
        },
        "e2": {
            "type": "base",
            "layout": {
                "x_point": "$x_pos",
                "y_point": "$y_pos",
                "width_point": 7,
                "height_point": 6,
                "r_point": 0.2
            },
            "fillCol": "#AFDFE4"
        }
    }
}