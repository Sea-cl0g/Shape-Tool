{
    "description": "Most basic and easy-to-see color theme",
    "author": "boo_manKnow",
    "containerAnker": "topLeft",
    "blockMode": "vertical",
    "blockAnker": "CENTER",
    "button_types": {
        "maru": "roundedSquareButton"
    },
    "fills": {
        "defaultCol": "#4a7083ff"
    },
    "layouts": {
        "default": {
            "x_point": 4,
            "y_point": 4,
            "width_point": 2,
            "height_point": 2,
            "r_point": 0.3
        }
    },
    "icons": {
        "main": {
            "size": 0.8,
            "path": "icon/TMP_ICON1.svg"
        }
    },
    "shadows": {
        "main": {
            "shadowMode": "BOTTOMRIGHT",
            "shadowDistPoint": 0.1,
            "color": "#ff1100"
        }
    },
    "stroke": {
        "main": {
            "stroke_point": 0,
            "color": "#42c8ed"
        }
    },
    "element1": {
        "type": "triggerButton",
        "style": [
            {
                "predicate": "normal",
                "button_type": "$maru",
                "layout": "$default",
                "fill": "$defaultCol",
                "icon": "$main"
            },
            {
                "predicate": "touched",
                "button_type": "$maru",
                "layout": "$default",
                "fill": "$defaultCol",
                "stroke": {
                    "stroke_point": 0.1,
                    "color": "#ffffff"
                },
                "icon": "$main",
                "shadow": "$main"
            },
            {
                "predicate": "clicked",
                "button_type": "$maru",
                "layout": "$default",
                "fill": "$defaultCol",
                "icon": "$main"
            }
        ]
    }
}