{
    "description": "It's the default theme pack! Hello!",
    "author": "boo_manKnow",
    "containerAnker": "topRight",
    "blockMode": "vertical",
    "blockAnker": "CENTER",
    "layer": 2,
    "button_types": {
        "maru": "squareButton"
    },
    "colors": {
        "defaultCol": "#4a7083ff"
    },
    "floats": {
        "x_pos": 4,
        "y_pos": 6,
        "b_size": 1.7
    },
    "layouts": {
        "buttonLayout": {
            "x_point": "$x_pos",
            "y_point": "$y_pos",
            "width_point": "$b_size",
            "height_point": "$b_size"
        }
    },
    "images": {
        "main": {
            "size": 0.8,
            "path": "icon/apple_stroke.svg",
            "color": "#ffffff"
        }
    },
    "shadows": {
        "main": {
            "shadowMode": "BOTTOMRIGHT",
            "shadowDistPoint": 0.2,
            "color": "#222222"
        }
    },
    "strokes": {
        "buttonStroke": {
            "stroke_point": 0.1,
            "color": "#4a7083ff"
        }
    },
    "elements": {
        "@FILL_BUTTON": {
            "type": "button",
            "style": [
                {
                    "predicate": "normal",
                    "button_type": "$maru",
                    "layout": "$buttonLayout",
                    "fillCol": "$defaultCol",
                    "stroke": "$buttonStroke",
                    "image": "$main"
                },
                {
                    "predicate": "touched",
                    "button_type": "$maru",
                    "layout": "$buttonLayout",
                    "fillCol": "$defaultCol",
                    "stroke": {
                        "stroke_point": 0.1,
                        "color": "#ffffff"
                    },
                    "image": "$main"
                },
                {
                    "predicate": "clicked",
                    "button_type": "$maru",
                    "layout": "$buttonLayout",
                    "fillCol": "$defaultCol",
                    "stroke": "$buttonStroke",
                    "image": "$main"
                }
            ]
        },
        "e1": {
            "type": "text_block",
            "layout": {
                "x_point": "$x_pos",
                "y_point": "$y_pos",
                "width_point": 6,
                "height_point": 1
            },
            "text": {
                "text": "stroke",
                "text_align": "LEFT",
                "text_size": 0.6,
                "text_col": "#FFFFFF"
            },
            "fillCol": "#FFFFFF00"
        }
    }
}