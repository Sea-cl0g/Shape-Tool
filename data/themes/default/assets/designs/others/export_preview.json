{
    "description": "Most basic and easy-to-see color theme",
    "author": "boo_manKnow",
    "containerAnker": "center",
    "blockMode": "vertical",
    "blockAnker": "CENTER",
    "layer": 1,

    "floats":{
        "x_pos": 0,
        "y_pos": -2,
        "w": 14,
        "h": 10,
        "r": 0.5
    },

    "elements": {
        "e1": {
            "type": "text_block",
            "layout": {
                "x_point": "$x_pos",
                "y_point": "$y_pos",
                "width_point": "$r * -3 + $w",
                "height_point": "$r * -3 + $h"
            },
            "text": {
                "text": "void setup(){|    size(500, 500);|}",
                "text_align": "LEFT",
                "text_size": 0.4,
                "text_col": "#000000"
            },
            "fillCol": "#FFFFFF00"
        },
        "e2": {
            "type": "base",
            "layout": {
                "x_point": "$x_pos",
                "y_point": "$y_pos",
                "width_point": "$r * -2 + $w",
                "height_point": "$r * -2 + $h",
                "tl_point": 0.6,
                "br_point": 0.6
            },
            "fillCol": "#FFFFFF"
        },
        "e3": {
            "type": "base",
            "layout": {
                "x_point": "$x_pos",
                "y_point": "$y_pos",
                "width_point": "$w",
                "height_point": "$h",
                "r_point": "$r"
            },
            "fillCol": "#6BA0CC"
        }
    }
}