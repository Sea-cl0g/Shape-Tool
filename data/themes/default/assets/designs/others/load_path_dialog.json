{
    "containerAnker": "topRight",
    "blockMode": "vertical",
    "blockAnker": "CENTER",
    "layer": 1,
    "colors": {
        "noFill": "#FFFFFF00"
    },
    "floats": {
        "x_pos": 5,
        "y_pos": 7,
        "w": 8,
        "h": 0.5
    },
    "elements": {
        "@MAIN_TEXT_EDITOR": {
            "type": "text_editor",
            "layout": {
                "x_point": "$x_pos",
                "y_point": "$h / 2 + $y_pos",
                "width_point": "$w",
                "height_point": "$h"
            },
            "text": {
                "text": "Text",
                "text_align": "LEFT",
                "text_size": 0.3,
                "text_col": "#ffffff"
            }
        },
        "e1": {
            "type": "base",
            "layout": {
                "x_point": "$x_pos",
                "y_point": "$h / 2 + $y_pos",
                "width_point": "$w + 0.5",
                "height_point": "$h + 0.2",
                "r_point": 0.2
            },
            "fillCol": "#222222",
            "stroke": {
                "stroke_point": 0.1,
                "color": "#222222"
            }
        }
    }
}