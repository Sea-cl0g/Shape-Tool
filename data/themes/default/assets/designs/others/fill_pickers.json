                "text_size": 0.6,
                "text_col": "#ffffff"
            }
        },
        "@FILL_RGB_B": {
            "type": "color_picker",
            "layout": {
                "x_point": "$x_pos",
                "y_point": "3 * $dist + $y_pos",
                "width_point": "$w",
                "height_point": "$h"
            }
        },
        "e3": {
            "type": "text_block",
            "layout": {
