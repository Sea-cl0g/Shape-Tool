{
    "containerAnker": "topRight",
    "blockMode": "vertical",
    "blockAnker": "CENTER",
    "layer": 1,
    "colors": {
        "noFill": "#FFFFFF00"
    },
    "floats": {
        "x_pos": 5,
        "y_pos": 3,
        "w": 8,
        "h": 1,
        "dist": 1.4
    },
    "elements": {
        "@FILL_RGB_R": {
            "type": "color_picker",
            "layout": {
                "x_point": "$x_pos",
                "y_point": "1 * $dist + $y_pos",
                "width_point": "$w",
                "height_point": "$h"
            }
        },
        "e1": {
            "type": "text_block",
            "layout": {
                "x_point": "$x_pos",
                "y_point": "1 * $dist + $y_pos",
                "width_point": "$w + 1.5",
                "height_point": "$h"
            },
            "text": {
                "text": "R:",
                "text_align": "LEFT",
                "text_size": 0.6,
                "text_col": "#ffffff"
            }
        },
        "@FILL_RGB_G": {
            "type": "color_picker",
            "layout": {
                "x_point": "$x_pos",
                "y_point": "2 * $dist + $y_pos",
                "width_point": "$w",
                "height_point": "$h"
            }
        },
        "e2": {
