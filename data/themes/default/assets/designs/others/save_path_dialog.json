{
    "description": "Most basic and easy-to-see color theme",
    "author": "boo_manKnow",
    "containerAnker": "center",
    "blockMode": "horizontal",
    "blockAnker": "CENTER",
    "layer": 10,
    "floats": {
        "w": 10,
        "h": 0.6,
        "label_x": 0,
        "label_y": -0.8,
        "label_shadow_pos": 0.02
    },
    "colors": {
        "text_box_back_col": "#FFFFFF"
    },
    "elements": {
        "e1": {
            "type": "text_block",
            "layout": {
                "x_point": "$label_x",
                "y_point": "$label_y",
                "width_point": 10,
                "height_point": 1
            },
            "text": {
                "text": "SAVE PATH",
                "text_align": "CENTER",
                "text_size": 0.4,
                "text_col": "$text_box_back_col"
            },
            "fillCol": "#FFFFFF00"
        },
        "e2": {
            "type": "text_block",
            "layout": {
                "x_point": "$label_x",
                "y_point": "$label_y + $label_shadow_pos",
                "width_point": 10,
                "height_point": 1
            },
            "text": {
                "text": "SAVE PATH",
                "text_align": "CENTER",
                "text_size": 0.4,
                "text_col": "#000000"
            },
            "fillCol": "#FFFFFF00"
        },
        "e3": {
            "type": "text_editor",
            "layout": {
                "x_point": 0,
                "y_point": 0,
                "width_point": "$w - 1",
                "height_point": "$h"
            },
            "text": {
                "text": "data/saves/new_project.json",
                "text_align": "LEFT",
                "text_size": 0.4,
                "text_col": "#000000"
            },
            "fillCol": "#FFFFFF00"
        },
        "e4": {
            "type": "base",
            "stroke": {
                "stroke_point": 0.08,
                "color": "#000000"
            },
            "layout": {
                "x_point": 0,
                "y_point": 0,
                "width_point": "$w",
                "height_point": "$h",
                "r_point": 0.1
            },
            "fillCol": "$text_box_back_col"
        }
    }
}