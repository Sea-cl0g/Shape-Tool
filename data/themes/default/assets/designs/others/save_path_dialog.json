{
    "containerAnker": "topLeft",
    "blockMode": "vertical",
    "blockAnker": "CORNER",
    "layer": 2,


    "floats": {
        "x_pos": 0,
        "y_pos": 0,
        "b_size": 1,
        "w": 10,
        "h": 0.7,
        "x_counter": 10
    },


    "elements": {
        "@MAIN_TEXT_EDITOR": {
            "type": "text_editor",
            "layout": {
                "x_point": "$x_counter * $b_size + $x_pos + 0.2",
                "y_point": "1 - $h / 2",
                "width_point": "$w",
                "height_point": "$h",
                "r_point": 0.2
            },
            "text": {
                "text": "LOAD",
                "text_align": "LEFT",
                "text_size": 0.4,
                "text_col": "#000000"
            }
        },
        "e1": {
            "type": "base",
            "layout": {
                "x_point": "$x_counter * $b_size + $x_pos",
                "y_point": "1 - $h / 2",
                "width_point": "$w",
                "height_point": "$h",
                "r_point": 0.2
            },
            "fillCol": "#ffffff"
        }
    }
}