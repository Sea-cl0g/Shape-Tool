{
    "description": "Most basic and easy-to-see color theme",
    "author": "boo_manKnow",
    "containerAnker": "topRight",
    "blockMode": "vertical",
    "blockAnker": "CENTER",
    "layer": 10,

    
    "colors": {
        "noFill": "#FFFFFF00"
    },

    "elements": {
        "@FILL_HSB_H": {
            "type": "color_picker",
            "layout": {
                "x_point": 7,
                "y_point": 8,
                "width_point": 6,
                "height_point": 0.8
            }
        },
        "@FILL_HSB_S": {
            "type": "color_picker",
            "layout": {
                "x_point": 7,
                "y_point": 9,
                "width_point": 6,
                "height_point": 0.8
            }
        },
        "@FILL_HSB_B": {
            "type": "color_picker",
            "layout": {
                "x_point": 7,
                "y_point": 10,
                "width_point": 6,
                "height_point": 0.8
            }
        },
        "e1": {
            "type": "text_block",
            "layout": {
                "x_point": 7,
                "y_point": 6.8,
                "width_point": 7,
                "height_point": 1
            },
            "text": {
                "text": "Fill Color",
                "text_align": "CENTER",
                "text_size": 0.6,
                "text_col": "#000000"
            },
            "fillCol": "$noFill"
        },
        "e2": {
            "type": "base",
            "layout": {
                "x_point": 7,
                "y_point": 9,
                "width_point": 7,
                "height_point": 6,
                "r_point": 0.4
            },
            "fillCol": "#d4dde1"
        }
    }
}