{
    "description": "Warm and welcoming design pack inspired by a Nordic village",
    "author": "boo_manKnow",
    "containerAnker": "center",
    "blockMode": "vertical",
    "blockAnker": "CENTER",
    "layer": 3,
    "button_types": {
        "maru": "roundedSquareButton"
    },
    "colors": {
        "buttonFill": "#FFFFFF00",
        "iconFill": "#FFFFFF",
        "iconTouched": "#ea5514"
    },
    "floats": {
        "b_size": 1
    },
    "layouts": {
        "buttonLayout": {
            "x_point": 5,
            "y_point": 6,
            "width_point": "$b_size",
            "height_point": "$b_size"
        }
    },
    "elements": {
        "@MAIN_BUTTON": {
            "type": "button",
            "style": [
                {
                    "predicate": "normal",
                    "button_type": "$maru",
                    "layout": "$buttonLayout",
                    "fillCol": "$buttonFill",
                    "image": {
                        "scale": 0.8,
                        "path": "icon/load.svg",
                        "color": "$iconFill"
                    }
                },
                {
                    "predicate": "touched",
                    "button_type": "$maru",
                    "layout": "$buttonLayout",
                    "fillCol": "$buttonFill",
                    "image": {
                        "scale": 0.8,
                        "path": "icon/load.svg",
                        "color": "$iconTouched"
                    }
                },
                {
                    "predicate": "clicked",
                    "button_type": "$maru",
                    "layout": "$buttonLayout",
                    "fillCol": "$buttonFill",
                    "image": {
                        "scale": 0.8,
                        "path": "icon/load.svg",
                        "color": "$buttonFill"
                    }
                }
            ]
        }
    }
}