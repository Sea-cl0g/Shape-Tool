{
    "containerAnker": "center",
    "blockMode": "both",
    "blockAnker": "CENTER",
    "layer": 1,
    "floats": {
        "y": -6,
        "shadow_dist": 0.1,
        "text_size": 2,
        "w": 11
    },
    "elements": {
        "e1": {
            "type": "text_block",
            "layout": {
                "x_point": 0,
                "y_point": "$y",
                "width_point": "$w",
                "height_point": "$text_size"
            },
            "text": {
                "text": "SETTINGS",
                "text_align": "CENTER",
                "text_size": "$text_size",
                "text_col": "#ffffff"
            },
            "fillCol": "#FFFFFF00"
        },
        "e2": {
            "type": "text_block",
            "layout": {
                "x_point": 0,
                "y_point": "$y + $shadow_dist",
                "width_point": "$w",
                "height_point": "$text_size"
            },
            "text": {
                "text": "SETTINGS",
                "text_align": "CENTER",
                "text_size": "$text_size",
                "text_col": "#000000"
            },
            "fillCol": "#FFFFFF00"
        },
        "e3": {
            "type": "base",
            "layout": {
                "x_point": 0,
                "y_point": "$text_size / 2 + $y + 0.4",
                "width_point": "$w",
                "height_point": "$shadow_dist"
            },
            "fillCol": "#FFFFFF"
        }
    }
}