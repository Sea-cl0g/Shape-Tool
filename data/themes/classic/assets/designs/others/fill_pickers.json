{
    "description": "It's the default theme pack! Hello!",
    "author": "boo_manKnow",
    "containerAnker": "topRight",
    "blockMode": "vertical",
    "blockAnker": "CENTER",
    "layer": 10,
    "colors": {
        "noFill": "#FFFFFF00"
    },
    "floats": {
        "x_pos": 8.5,
        "y_pos": 5
    },
    "elements": {
        "@FILL_HSB_H": {
            "type": "color_picker",
            "layout": {
                "x_point": "$x_pos",
                "y_point": "$y_pos - 1",
                "width_point": 6,
                "height_point": 0.8
            }
        },
        "@FILL_HSB_S": {
            "type": "color_picker",
            "layout": {
                "x_point": "$x_pos",
                "y_point": "$y_pos",
                "width_point": 6,
                "height_point": 0.8
            }
        },
        "@FILL_HSB_B": {
            "type": "color_picker",
            "layout": {
                "x_point": "$x_pos",
                "y_point": "$y_pos + 1",
                "width_point": 6,
                "height_point": 0.8
            }
        },
        "@FILL_ALPHA": {
            "type": "color_picker",
            "layout": {
                "x_point": "$x_pos",
                "y_point": "$y_pos + 2",
                "width_point": 6,
                "height_point": 0.8
            }
        },
        "e1": {
            "type": "text_block",
            "layout": {
                "x_point": "$x_pos",
                "y_point": "$y_pos - 3 + 0.8",
                "width_point": 7,
                "height_point": 1
            },
            "text": {
                "text": "Fill Color",
                "text_align": "CENTER",
                "text_size": 0.6,
                "text_col": "#000000"
            },
            "fillCol": "$noFill"
        },
        "e2": {
            "type": "base",
            "layout": {
                "x_point": "$x_pos",
                "y_point": "$y_pos",
                "width_point": 7,
                "height_point": 6,
                "r_point": 0.3
            },
            "fillCol": "#d4dde1"
        },
        "e3": {
            "type": "base",
            "layout": {
                "x_point": "$x_pos - 0.2",
                "y_point": "$y_pos + 0.2",
                "width_point": 7,
                "height_point": 6,
                "r_point": 0.3
            },
            "fillCol": "#222222"
        }
    }
}