{
    "description": "It's the default theme pack! Hello!",
    "author": "boo_manKnow",
    "containerAnker": "center",
    "blockMode": "vertical",
    "blockAnker": "CENTER",
    "layer": 0,

    "floats":{
        "x": -4,
        "w": 12
    },

    "elements": {
        "e1": {
            "type": "canvas",
            "layout": {
                "x_point": "$x",
                "y_point": 0,
                "width_point": "$w",
                "height_point": "$w"
            },
            "fillCol": "#FFFFFF"
        },
        "e2": {
            "type": "easel",
            "layout": {
                "x_point": "$x",
                "y_point": 0,
                "width_point": "$w + 0.2",
                "height_point": "$w + 0.2"
            },
            "fillCol": "#AAAAAA"
        }
    }
}