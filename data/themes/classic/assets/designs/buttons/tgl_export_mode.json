{
    "containerAnker": "bottomRight",
    "blockMode": "vertical",
    "blockAnker": "CORNER",
    "layer": 2,
    "colors": {
        "defaultCol": "#4a7083ff"
    },
    "layouts": {
        "buttonLayout": {
            "x_point": 0.5,
            "y_point": 3,
            "width_point": 1.6,
            "height_point": 1.6,
            "r_point": 0.3
        }
    },
    "images": {
        "main": {
            "scale": 0.6,
            "path": "icon/processing4.svg",
            "color": "#ffffff"
        }
    },
    "shadows": {
        "main": {
            "shadowMode": "BOTTOMRIGHT",
            "shadowDistPoint": 0.2,
            "color": "#222222"
        }
    },
    "strokes": {
        "buttonStroke": {
            "stroke_point": 0,
            "color": "#42c8ed"
        }
    },
    "elements": {
        "@MAIN_BUTTON": {
            "type": "button",
            "style": [
                {
                    "predicate": "normal",
                    "layout": "$buttonLayout",
                    "fillCol": "$defaultCol",
                    "image": "$main",
                    "shadow": "$main"
                },
                {
                    "predicate": "touched",
                    "layout": "$buttonLayout",
                    "fillCol": "$defaultCol",
                    "stroke": {
                        "stroke_point": 0.1,
                        "color": "#ffffff"
                    },
                    "image": "$main",
                    "shadow": "$main"
                },
                {
                    "predicate": "clicked",
                    "layout": "$buttonLayout",
                    "fillCol": "$defaultCol",
                    "image": "$main",
                    "shadow": "$main"
                }
            ]
        }
    }
}