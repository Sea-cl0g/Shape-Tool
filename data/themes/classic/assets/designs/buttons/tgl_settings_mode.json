{
    "description": "It's the default theme pack! Hello!",
    "author": "boo_manKnow",
    "containerAnker": "topRight",
    "blockMode": "vertical",
    "blockAnker": "CORNER",
    "layer": 2,
    "colors": {
        "defaultCol": "#ffffff22",
        "touchedCol": "#ffffff11",
        "clickedCol": "#ffffff33"
    },
    "floats": {
        "baseW": 10,
        "count": 0,
        "th": 0.1,
        "h": 1
    },
    "layouts": {
        "defaultLayout": {
            "x_point": "$baseW / 5 * $count",
            "y_point": 1,
            "width_point": "$baseW / 5",
            "height_point": "$h"
        },
        "touchedLayout": {
            "x_point": "$baseW / 5 * $count",
            "y_point": 1,
            "width_point": "$baseW / 5",
            "height_point": "$h - $th"
        }
    },
    "images": {
        "main": {
            "scale": 0.6,
            "path": "icon/setting.svg",
            "color": "#ffffff"
        }
    },
    "shadows": {
        "defaultShadow": {
            "shadowMode": "BOTTOM",
            "shadowDistPoint": 0.1,
            "color": "#222222"
        }
    },
    "elements": {
        "@MAIN_BUTTON": {
            "type": "button",
            "style": [
                {
                    "predicate": "normal",
                    "layout": "$defaultLayout",
                    "fillCol": "$defaultCol",
                    "shadow": "$defaultShadow",
                    "image": "$main"
                },
                {
                    "predicate": "touched",
                    "layout": "$defaultLayout",
                    "fillCol": "$touchedCol",
                    "shadow": "$defaultShadow",
                    "image": "$main"
                },
                {
                    "predicate": "clicked",
                    "layout": "$touchedLayout",
                    "fillCol": "$clickedCol",
                    "image": "$main"
                }
            ]
        }
    }
}